___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const createQueue = require('createQueue');
const encodeUriComponent = require("encodeUriComponent");
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const setInWindow = require('setInWindow');
const trafficguardConversionTag = data.trafficguard_conversion_tag;
const trafficguardConversionEvent = data.trafficguard_conversion_event;
const trafficguardTransactionId = data.transaction_id;
const trafficguardConversionValue = data.conversion_value;
const trafficguardConversionCurrency = data.conversion_currency;
const isArray = arr => callInWindow('toString.call', arr) === '[object Array]';

const dataTrafficGuard = copyFromWindow('dataTrafficGuard');

let conversionParameters = {};

if (!isArray(dataTrafficGuard)) {
setInWindow('dataTrafficGuard', dataTrafficGuard ? [dataTrafficGuard] : [], true);
}

// mandatory value
conversionParameters.tag = trafficguardConversionTag;

// build conversion event object: value / currency
if ((trafficguardConversionValue != undefined) && (trafficguardConversionCurrency != '-')) {
conversionParameters.conversion_value = trafficguardConversionValue;
conversionParameters.currency_code = trafficguardConversionCurrency;
}

// build conversion event object: transaction_id
if (trafficguardTransactionId != undefined) {
conversionParameters.transaction_id = trafficguardTransactionId;
}

log(conversionParameters);

// property group id starts with tg-g
if (1 === 1) {
log('Sending conversion');
callInWindow('dataTrafficGuard.push', ['event', trafficguardConversionEvent, conversionParameters]);
log('TrafficGuard Conversion Tag loaded successfully.');
data.gtmOnSuccess();
}


